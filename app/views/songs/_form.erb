<%= form_for(@song) do |f| %>
    <div>
    <%= f.label "Title" %>
    <%= f.text_field :title %>
    </div>

    <div> 
    <%= f.label "Released Status" %>
    <%= f.check_box :released %> 
    </div>

    <div>
    <%= f.label "Release year" %>
    <%= f.text_field :release_year %>
    </div> 
 
  <div>
    <%= f.label "Artist" %> 
    <%= f.text_field :artist_name %>
    </div>

    <div>
    <%= f.label "Genre" %> 
    <%= f.text_field :genre %>
    </div>

  
    <%= f.submit %>
  <% end %>

