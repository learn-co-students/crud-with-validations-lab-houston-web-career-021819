<%= form_for @song do |f| %>
    Title: <%=f.text_field :title, name: 'song[title]'%>
    Release Year: <%=f.text_field :release_year, name: 'song[release_year]'%>
    Artist Name: <%=f.text_field :artist_name, name: 'song[artist_name]'%>
    Genre: <%=f.text_field :genre, name: 'song[genre]'%>
    Released? <%=f.check_box :released, name: 'song[released]'%> 
    <%=f.submit%>
<% end %>%>