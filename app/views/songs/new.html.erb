<%= form_for(@song) do | form | %>
    <%= form.label "Title"%>
    <%= form.text_field :title%><br>
    <%= form.label "Released"%>
    <%= form.check_box :released, {checked:true}%><br>
    <%= form.label "Release Year"%>
    <%= form.text_field :release_year%><br>
    <%= form.label "Artist Name"%>
    <%= form.text_field :artist_name%><br>
    <%= form.label "Genre"%>
    <%= form.text_field :genre%><br>
    <%= form.submit %>
<% end %>